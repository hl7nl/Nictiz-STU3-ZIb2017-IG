           ] ) ;
           fhir:text [ fhir:v "Irma Jongeneel-de Haas" ] ;
           fhir:family [
             fhir:v "Jongeneel-de Haas" ;
             fhir:extension ( [
               fhir:url [ fhir:v "http://hl7.org/fhir/StructureDefinition/humanname-own-name"^^xsd:anyURI ] ;
               fhir:value [ fhir:v "Jongeneel" ]
             ] [
