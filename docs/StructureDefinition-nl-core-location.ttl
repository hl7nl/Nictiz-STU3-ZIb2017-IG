         fhir:value [
           a fhir:Address ;
           fhir:extension ( [
             fhir:url [ fhir:v "http://fhir.nl/fhir/StructureDefinition/nl-core-address-official"^^xsd:anyURI ] ;
             fhir:value [ fhir:v "true"^^xsd:boolean ]
           ] ) ;
           fhir:use [ fhir:v "home" ] ;
           fhir:line ( [
             fhir:v "Binnenkant to 10" ;
